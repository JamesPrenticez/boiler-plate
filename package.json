{
  "name": "boiler_plate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "lint": "eslint . --ignore-path .eslintignore --ext js,ts,jsx,tsx --fix",
    "lint-check": "eslint . --ignore-path .eslintignore --ext js,ts,jsx,tsx",
    "format": "prettier --write \"./**/*.{css,scss,json,md,js,ts,tsx,jsx}\"",
    "format-check": "prettier --check \"./**/*.{css,scss,json,md,js,ts,tsx,jsx}\"",
    "scss-lint": "stylelint \"./**/*.{css,scss}\"",
    "scss-lint-fix": "stylelint \"./**/*.{css,scss}\" --fix",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@tanstack/react-query": "^4.29.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7.20.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "country-locale-map": "^1.8.12",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.31.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
